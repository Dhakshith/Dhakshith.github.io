{
    "manifest_version": 2,
    "name": "Popping - Up Clock",
    "version": "0.1",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "Sketch.js"
            ]
        }
    ],
    "browser_action": {
        "default_icon": "032 Icon.png",
        "default_popup": "Clock/index.html",
        "default_title": "Clock"
    },
    "icons": { 
         "16": "016 Icon.png",
         "32": "032 Icon.png",
         "48": "048 Icon.png",
        "128": "128 Icon.png"
    }
}